{
  "name": "brs-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:production": "npm run lint && npm run build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 200",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test:build": "npm run build && npm run preview",
    "deploy": "chmod +x deploy.sh && ./deploy.sh",
    "deploy:production": "NODE_ENV=production npm run build:production && chmod +x deploy.sh && ./deploy.sh",
    "rollback": "chmod +x rollback.sh && ./rollback.sh",
    "setup:aws": "cd aws-configs && chmod +x setup-aws.sh && ./setup-aws.sh",
    "analyze": "npm run build && npx vite-bundle-analyzer dist/",
    "clean": "rm -rf dist node_modules/.vite",
    "prebuild": "npm run clean && npm run type-check",
    "terraform:init": "cd aws-configs && terraform init",
    "terraform:plan": "cd aws-configs && terraform plan",
    "terraform:apply": "cd aws-configs && terraform apply",
    "terraform:destroy": "cd aws-configs && terraform destroy",
    "terraform:output": "cd aws-configs && terraform output",
    "infra:setup": "npm run terraform:init && npm run terraform:apply",
    "infra:deploy": "npm run build:production && npm run terraform:get-bucket && npm run terraform:sync && npm run terraform:invalidate",
    "terraform:get-bucket": "cd aws-configs && terraform output -raw s3_bucket_name > ../.bucket-name",
    "terraform:sync": "powershell \"aws s3 sync dist/ s3://$(Get-Content .bucket-name)/ --delete\"",
    "terraform:invalidate": "cd aws-configs && powershell \"aws cloudfront create-invalidation --distribution-id $(terraform output -raw cloudfront_distribution_id) --paths '/*'\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^3.3.2",
    "@mui/icons-material": "^5.15.0",
    "@mui/lab": "^5.0.0-alpha.170",
    "@mui/material": "^5.15.0",
    "@reduxjs/toolkit": "^2.0.1",
    "axios": "^1.6.2",
    "date-fns": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.1",
    "yup": "^1.3.3"
  },
  "devDependencies": {
    "@types/node": "^24.3.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.1.1",
    "terser": "^5.44.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
